package test;

public class Login_StepDef {
	
	 @Given("^user has opened the amazon on the chrome browser$")
	    public void user_has_opened_the_amazon_on_the_chrome_browser() throws Throwable {
	        throw new PendingException();
	    }

	    @When("^the user enters the correct username and password$")
	    public void the_user_enters_the_correct_username_and_password() throws Throwable {
	        throw new PendingException();
	    }

	    @When("^the user enters the incorrect username and password$")
	    public void the_user_enters_the_incorrect_username_and_password() throws Throwable {
	        throw new PendingException();
	    }

	    @Then("^user should be able to login successfully$")
	    public void user_should_be_able_to_login_successfully() throws Throwable {
	        throw new PendingException();
	    }

	    @Then("^user should be not able to login successfully$")
	    public void user_should_be_not_able_to_login_successfully() throws Throwable {
	        throw new PendingException();
	    }

	}

}
