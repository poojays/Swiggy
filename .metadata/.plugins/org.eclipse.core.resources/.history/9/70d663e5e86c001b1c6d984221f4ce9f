package test;

public class EvenNum {

	public static void main(String[] args) {
	int num=10;
	boolean flag=false;
	for(int i=2;i<=num/2;i++) {
	
	 if(num%i==0){
		 flag=true;
		
	 }
	}
	if(!flag)
	
		System.out.println("Prime Number");
	
	else
	
		System.out.println("Not a prime number");
	}
	}
	


